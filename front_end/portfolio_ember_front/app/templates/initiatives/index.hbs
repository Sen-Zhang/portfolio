{{outlet}}

<h3>Initiatives</h3>

<table class="table table-striped table-hover ">
  <thead>
  <tr>
    <th>ID</th>
    <th>Title</th>
    <th>Time Period</th>
    <th>Target Population</th>
    <th>Employee Number</th>
    <th>Target Countries</th>
    <th>Language</th>
    <th>Launch Date</th>
  </tr>
  </thead>
  <tbody>
  {{#each model as |i|}}
    <tr>
      <td>{{i.id}}</td>
      <td>{{#link-to 'initiatives.show' i.id}}{{i.title}}{{/link-to}}</td>
      <td>{{i.timePeriod}}</td>
      <td>{{i.population}}</td>
      <td>{{i.employeeNum}}</td>
      <td>{{i.countries}}</td>
      <td>{{i.language}}</td>
      <td>{{format-date i.launchDate}}</td>
    </tr>
  {{/each}}
  </tbody>
</table>